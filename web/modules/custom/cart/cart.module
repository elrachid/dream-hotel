<?php

/**
 * @file
 * Provides custom functionality for the Cart module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function cart_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.cart':
      return '<p>' . t('Help text for the Cart module.') . '</p>';
  }
}
